# WolfeUp Platform Environment Variables
# Copy to .env and fill in values

# === Authentication ===
JWT_SECRET=your_jwt_secret_here

# === Reply Orchestrator ===
REPLY_ORCHESTRATOR_TOKEN=your_reply_orchestrator_token
REPLY_ORCHESTRATOR_URL=http://localhost:7890

# === Ollama / LLM ===
OLLAMA_PRIMARY_URL=http://localhost:11434
OLLAMA_FALLBACK_URL=http://localhost:11434

# === MCP Servers ===
MCP_HOST=localhost

# === Server IPs (internal network) ===
GATEWAY_HOST=10.0.10.10
NODE_HOST=10.0.10.11
WEB_HOST=10.0.10.12
GPU_HOST=192.168.1.70
NAS_HOST=192.168.1.2

# === External API Keys ===
BRAVE_API_KEY=your_brave_api_key
GITHUB_TOKEN=your_github_token

# === Chrome CDP ===
CDP_ENDPOINT=ws://localhost:18800

# === Deploy ===
DEPLOY_WEBHOOK_SECRET=your_webhook_secret
